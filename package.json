    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix src/**/*.{ts,tsx,js,jsx}",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "test:lint": "next lint",
    "test:prettier": "prettier --check ./src",
    "prepare": "husky",
    "ts:check": "bunx tsc --noEmit",
